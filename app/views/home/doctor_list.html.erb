<div class="page-header">
  
  <h1>Doctors Clinic</h1>
</div>
<table class="table table-striped table-bordered" id="dockolist">
  <thead>
    <tr>
      <th>Clinic Name</th>
      <th>Address</th>
      <th>Contact#</th>
    </tr>
  </thead>
  <tbody>
    <% @doctor_clinics.each do |doc_kol| %>
      <tr>   
        <td><%= doc_kol.name %></td>
        <td><%= doc_kol.address %></td>
        <td>

         <%doc_kol.contact_no.split(',').each do |f|%>
           <%#if f[i].present?%>
             <%=f%><br>
           <%#end%>
         <%end%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<script>
$(function() {    
    $('#dockolist').dataTable();
 
 
 
});


</script>

